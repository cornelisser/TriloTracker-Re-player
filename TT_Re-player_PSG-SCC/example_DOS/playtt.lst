      47.  00:103D                      scci_test:
      48.  00:103D  21 00 B0                             ld hl,#b000
      49.  00:1040  46                                   ld b,(hl)         ; save contents of address 9000h
      50.  00:1041  36 3F                                ld (hl),#3f       ; activate SCC (if present)
      51.  00:1043  26 9C                                ld h,#9c          ; address of SCC-register mirrors
